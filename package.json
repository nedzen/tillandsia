    "eslint": "^7.19.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.19.0",
    "fathom-client": "^3.4.1",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
