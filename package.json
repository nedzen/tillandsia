    "react-dom": "^17.0.2",
    "react-photoswipe-gallery": "^2.2.6",
    "react-spring-lightbox": "^1.7.1",
    "rehype-autolink-headings": "^5.0.1",
    "rehype-slug": "^4.0.1",
    "remark-code-titles": "^0.1.1",
